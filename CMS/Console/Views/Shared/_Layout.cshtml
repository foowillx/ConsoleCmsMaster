<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Dashboard">
    <meta name="keyword" content="Dashboard, Bootstrap, Admin, Template, Theme, Responsive, Fluid, Retina">

    <title>Nutrir Console</title>

    <!-- Bootstrap core CSS -->
    @Styles.Render("~/Content/Console/assets/css/bootstrap.css")
    <!--external css-->
    @Styles.Render("~/Content/Console/assets/font-awesome/css/font-awesome.css")

    <!-- Custom styles for this template -->
    @Styles.Render("~/Content/Console/assets/css/style.css")
    @Styles.Render("~/Content/Console/assets/css/style-responsive.css")

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
    @*@Styles.Render("~/Content/css")*@
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <style id="antiClickjack">
        body {
            display: none !important;
        }
    </style>
    <script type="text/javascript">
    if (self == top) {
        var antiClickjack = document.getElementById("antiClickjack");
        antiClickjack.parentNode.removeChild(antiClickjack);
    } else {
        top.location = self.location;
    }
    </script>

    <script src="~/Bnn_Scripts/BNN_Jquery.js"></script>
    <script src="~/Bnn_Scripts/highcharts.js"></script>
    <script src="~/Bnn_Scripts/exporting.js"></script>
    <section id="container">

        <header class="header black-bg">
            <div class="sidebar-toggle-box">
                <div class="fa fa-bars tooltips" data-placement="right" data-original-title="Toggle Navigation"></div>
            </div>
            <!--logo start-->
            <a href="@Url.Action("Index","Home")" class="logo"><b>NUTRIR</b></a>
            <!--logo end-->
            <div class="top-menu">
                <ul class="nav pull-right top-menu">
                    <li><a class="logout" href="@Url.Action("Logout","Home")">Logout</a></li>
                </ul>
            </div>
        </header>
        <!--header end-->
        <!--sidebar start-->
        <aside>
            <div id="sidebar" class="nav-collapse ">
                <!-- sidebar menu start-->
                @if (Session["Info2"].ToString() == "Administrador")
                {
                    @Html.Partial("Partials/_SideBar")
                }
                else
                {
                    @Html.Partial("Partials/_SideBarMinus")
                }
                
                <!-- sidebar menu end-->
            </div>
        </aside>
        <!--sidebar end-->
        <!--main content start-->
        <section id="main-content">
            <section class="wrapper site-min-height mamalona">
                @RenderBody()

            </section><! --/wrapper -->
        </section><!-- /MAIN CONTENT -->
        <!--main content end-->
        <!--footer start-->
        <footer class="site-footer">
            <div class="text-center">
                2017 - Nutrir
                <a href="blank.html#" class="go-top">
                    <i class="fa fa-angle-up"></i>
                </a>
            </div>
        </footer>
        <!--footer end-->

    </section>


    <!-- js placed at the end of the document so the pages load faster -->
    
    <script src="~/Content/Console/assets/js/jquery.customSelect.min.js"></script>
    <script src="~/Content/Console/assets/js/bootstrap.min.js"></script>
    <script src="~/Content/Console/assets/js/jquery-ui-1.9.2.custom.min.js"></script>
    <script src="~/Content/Console/assets/js/jquery.ui.touch-punch.min.js"></script>
    <script class="include" type="text/javascript" src="~/Content/Console/assets/js/jquery.dcjqaccordion.2.7.js"></script>
    <script src="~/Content/Console/assets/js/jquery.scrollTo.min.js"></script>
    <script src="~/Content/Console/assets/js/jquery.nicescroll.js" type="text/javascript"></script>


    <!--common script for all pages-->
    <script src="~/Content/Console/assets/js/common-scripts.js"></script>

    <!--script for this page-->

    <script>
        //custom select box

        $(function () {
            $('select.styled').customSelect();
        });

    </script>
    <script>
        var timeout = (20 * 60000);
        setTimeout(function () {
            window.location.href = "@Url.Action("Logout", "Home")";
        }, timeout);
    </script>
</body>
</html>
